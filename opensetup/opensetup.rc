#include <windows.h>
#include "resource.h"

IDI_APPLICATION_LARGE ICON "res\\appicon_l.ico"
IDI_APPLICATION_SMALL ICON "res\\appicon_s.ico"

IDB_IMAGELIST BITMAP "res\\imagelist.bmp"
IDB_IMAGELIST_MASK BITMAP "res\\imagelist-mask.bmp"

IDD_MAIN_DIALOG DIALOGEX 0, 0, 250, 205
STYLE DS_MODALFRAME | DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
EXSTYLE WS_EX_APPWINDOW | WS_EX_CONTROLPARENT
CAPTION "RagnarokOnline OpenSetup"
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
FONT 8, "Tahoma"
BEGIN
    CONTROL "Ok", IDOK, BUTTON, BS_DEFPUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 85, 185, 50, 14
    CONTROL "Cancel", IDCANCEL, BUTTON, BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 140, 185, 50, 14
    CONTROL "Apply", IDAPPLY, BUTTON, BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 195, 185, 50, 14
END

IDD_TAB_VIDEO DIALOG 0, 15, 250, 170
STYLE WS_CHILD | WS_VISIBLE
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
FONT 8, "Tahoma"
BEGIN
    CONTROL "Graphic Settings", -1, BUTTON, BS_GROUPBOX | WS_CHILD | WS_VISIBLE | WS_GROUP, 5, 5, 240, 160
    CONTROL "Graphic Device:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 15, 20, 54, 9
    CONTROL "", IDCOMBOBOX_VIDEODEVICE, COMBOBOX, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_VSCROLL | WS_TABSTOP, 15, 30, 150, 100
    CONTROL "Resolution:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 15, 50, 36, 9
    CONTROL "", IDCOMBOBOX_RESOLUTION, COMBOBOX, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_VSCROLL | WS_TABSTOP, 15, 60, 150, 100
    CONTROL "Options:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 15, 80, 33, 9
    CONTROL "Play in Full Screen", IDCHECKBOX_FULLSCREEN, BUTTON, BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 15, 90, 72, 10
    CONTROL "Enable Fog", IDCHECKBOX_FOG, BUTTON, BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 90, 90, 52, 10
    CONTROL "Use Lightmap", IDCHECKBOX_LIGHTMAP, BUTTON, BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 145, 90, 59, 10
    CONTROL "Sprite Quality:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 15, 105, 49, 8
    CONTROL "Slider2", IDTRACKBAR_SPRITEQ, "msctls_trackbar32", TBS_HORZ | TBS_AUTOTICKS | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 15, 115, 80, 12
    CONTROL "Texture Quality:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 110, 105, 55, 8
    CONTROL "Slider1", IDTRACKBAR_TEXTUREQ, "msctls_trackbar32", TBS_HORZ | TBS_AUTOTICKS | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 110, 115, 80, 12
    CONTROL "Troubleshooting:", -1, STATIC, SS_LEFT | WS_CHILD | WS_VISIBLE, 15, 135, 58, 9
    CONTROL "VooDoo3 graphics card installed", IDCHECKBOX_VOODOO, BUTTON, BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 15, 145, 117, 10
END

